<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Spinner
        android:id="@+id/spn_birth"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:entries="@array/birth"
        tools:ignore="MissingConstraints">
    </Spinner>
    <Spinner
        android:id="@+id/spn_address"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:entries="@array/address"
        tools:ignore="MissingConstraints">
    </Spinner>
    <Spinner
        android:id="@+id/spn_school"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:entries="@array/school"
        tools:ignore="MissingConstraints">
    </Spinner>
    <Spinner
        android:id="@+id/spn_subject"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:entries="@array/subject"
        tools:ignore="MissingConstraints">
    </Spinner>
    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="별명을 설정해주세요."
        android:id="@+id/et_mem_lay"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="설정하기"
        android:layout_marginTop="30dp"
        android:id="@+id/bt_member"
        />
</LinearLayout>